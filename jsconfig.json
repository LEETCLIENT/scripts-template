{
  "include": ["**/*.js", "leet.d.ts"],
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2022"],
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "baseUrl": ".",
    "noEmit": true,
    "strictNullChecks": true
  }
}
